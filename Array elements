#include <iostream>

using namespace std;

int main()
{
    int i,a[10],x;
    cout<<"Enter number of elements less than 10:";
    cin>>x;
    cout<<"Enter elements:";
    for (i=0;i<x;i++)
    {
        cin>>a[i];
    }
    cout<<"The elements are:";
    for(i=0;i<x;i++)
    {
        cout<<a[i]<<endl;
    }
    return 0;
}
